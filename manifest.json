{
    "manifest_version": 3,
    "name": "CityU Quick Course Adder",
    "description": "To help you fill the CRNs quickly.",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["http://*/*/bwskfreg.P_AltPin*", "https://*/*/bwskfreg.P_AltPin*"],
          "js": ["contentScript.js"]
        }
      ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}